<?xml version="1.0"?>
<odoo>
  <template id="terralab.common_header">
    <!-- Report header -->
    <div class="terralab-title bg-primary p-3 mt-3">
      <div class="row">
        <div class="col-6 text-left">
          TerraLab <t t-raw="0"/>
        </div>
        <div class="col-6 text-right">
          <span t-esc="context_timestamp(datetime.datetime.now()).strftime('%Y-%m-%d %H:%M')"/>
        </div>
      </div>
    </div>
    <!-- End report header -->
  </template>
</odoo>
